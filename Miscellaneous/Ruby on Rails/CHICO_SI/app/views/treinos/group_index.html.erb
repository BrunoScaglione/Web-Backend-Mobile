<h1>Lista de treinos por grupo</h1>

<% current_user.groups.each do |group| %>
  <ul class="listing">
    <div class="row">
      <div class="well col-md-4 col-md-offset-4">
        <li>
          <%= link_to group.nome, treino_group_path(group) %>
        </li>
      </div>
    </div>
  </ul>
<% end %>


<%= link_to 'Voltar', home_path %>