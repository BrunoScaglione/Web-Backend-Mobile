<h1>Lista de planos</h1>

<% @planos.each do |plano| %>
  <ul class="listing">
    <div class="row">
      <div class="well col-md-4 col-md-offset-4">
        <li>
          <%= link_to plano.nome, plano_path(plano) %>
        </li>
      </div>
    </div>
  </ul>
<% end %>

<%= link_to 'Criar plano', new_plano_path %>
<%= link_to 'Voltar', home_path %>